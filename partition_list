# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def partition(self, head: Optional[ListNode], x: int) -> Optional[ListNode]:
        h1 = l1 = ListNode(0)
        h2 = l2 = ListNode(0)

        current = head
        while current:
            if current.val < x:
                l1.next = current
                l1 = l1.next
            else:
                l2.next = current
                l2 = l2.next
            current = current.next
        l2.next = None  
        l1.next = h2.next

        return h1.next
        
